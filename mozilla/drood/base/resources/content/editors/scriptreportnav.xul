<?xml-stylesheet href="chrome://drood/skin/" type="text/css"?>

<!DOCTYPE window [
#include ../drood-entities.inc
]>

<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
        id="scriptreportnav"
        title="Window Title"
        onload="loaded()">

#include ../drood-scripts.inc
  <script type="application/x-javascript"
          src="chrome://drood/content/editors/scriptreportnav.js"/>

  <vbox class="droodtoolbar"
        flex="1">
    <hbox class="panelheader" align="center">
      <label value="&reports.label;"/>
    </hbox>

    <tree id="report-tree"
          flex="1"
          seltype="single"
          onselect="goDoCommand('cmd-report-changed')"
          hidecolumnpicker="true">
      <treecols>
        <treecol id="report-type-col"
                 hideheader="true"
                 flex="1"/>
      </treecols>
      <treechildren>
        <treeitem>
          <treerow>
            <treecell label="&sceneBreakdown.label;"
                      tooltiptext="&sceneBreakdown.label;"/>
          </treerow>
        </treeitem>
        <treeitem>
          <treerow>
            <treecell label="&sceneBreakdownWithDesc.label;"
                      tooltiptext="&sceneBreakdownWithDesc.label;"/>
          </treerow>
        </treeitem>
        <treeitem>
          <treerow>
            <treecell label="&dialog.label;"
                      tooltiptext="&dialog.label;"/>
          </treerow>
        </treeitem>
        <treeitem>
          <treerow>
            <treecell label="&elements.label;"
                      tooltiptext="&elements.label;"/>
          </treerow>
        </treeitem>
        <treeitem>
          <treerow>
            <treecell label="&scriptWithNotes.label;"
                      tooltiptext="&scriptWithNotes.label;"/>
          </treerow>
        </treeitem>
      </treechildren>
    </tree>
  </vbox>

</window>

